                                                    <script type="text/javascript" charset="utf-8">
    function isNumber(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode;
        if (charCode > 31 && (charCode < 46 || charCode > 57))
            return false;
        return true;
    }

    function isAlpha(evt) {
        // onkeypress="return isAlpha(event)" 
        var charCode = (evt.which) ? evt.which : event.keyCode;
        if (charCode > 31 && (charCode < 65 || charCode > 90) && (charCode < 97 || charCode > 122))
            return false;

        return true;
    }
    $(document).ready(function() {
        $('#example').dataTable({"sDom": 'T<"clear">lfrtip', "oTableTools": {"sSwfPath": "<?php echo $this->baseUrl(); ?>/public/media/swf/copy_csv_xls_pdf.swf"}});
         $("#grn_date").live('focus',function() { $("#grn_date").datepick({dateFormat: 'dd-mm-yyyy'});  });
    });

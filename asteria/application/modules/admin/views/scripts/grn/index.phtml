            $(document).on("click", ".serialgen_edit", function(){
                var id = $(this).attr("id");
                var id = id.split("_"); 
                var id = id[1];
                var itemid =    $('#item_'+id).val();
                var accepted =  $('#ok_'+id).val();
                var okqty =     $('#okqty_'+id).val();
                var vendorid =  $('.vendor_name').attr("value");
                var serial = $('#uniquesl_'+id).val();
                //alert(vendorid);
//                alert(okqty);
                var count = parseInt(okqty)-parseInt(accepted);
              //  alert(count)
                if(count < 0){
                    // To Del
                        alert('to del');
                        var remainsl = parseInt(accepted)-parseInt(okqty);
                     var sls = serial.split(",");
                  //   alert(sls.length);
                     var slslength = sls.length;
                    var count = slslength - remainsl;
                   // alert(count);
                    var showsl = '';
                    var delsl = '';
                    for(var i=0; i<count; i++){
                        showsl += sls[i]+',';
                    }
                   var showsl = showsl.slice(0,-1);
                    $('#serial_'+id).val(showsl);
                   // To del sl no 
                    for(var i=count+1; i<=slslength; i++){
                      delsl +=sls[i-1]+',';
                    }
                    var delsl = delsl.slice(0,-1);
                    $('#toadd_'+id).val('');
                    $('#todel_'+id).val(delsl);
                }
                else{
                    // To add
                 //   alert('to add');
                    var dataString ='&type=generateuniqueno&vendorid='+vendorid+'&itemid='+itemid+'&okqty='+count+'&serial='+serial;
                                        $.ajax({

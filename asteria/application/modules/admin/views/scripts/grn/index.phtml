   $(document).ready(function(){
       
         function okcalc(id){
               var rcvd = $('#rcvdqty_'+id).val();
               var okqty = $('#okqty_'+id).val();
               
               var rejected = parseInt(rcvd) - parseInt(okqty);
               $('#rejectedqty_'+id).val(rejected);
               if(parseInt(okqty) > parseInt(rcvd)){
                  $('#okqty_'+id).focus();
                  alert("Accepted Qty is greater than Received");
                  return false;
              }
              else{
                  return true;
              }
              
          }
          
          
        function rcvdcalc(id){
               var poqty = $('#poqty_'+id).val();
               var rcvd = $('#rcvdqty_'+id).val();
//               alert(poqty);
//               alert(rcvd);
              if(parseInt(rcvd) > parseInt(poqty)){
                  $('#rcvdqty_'+id).focus();
                  alert("Received Qty is greater than PO Qty");
                  return false;
              }
              else{
                  return true;
              }
          }
          
          

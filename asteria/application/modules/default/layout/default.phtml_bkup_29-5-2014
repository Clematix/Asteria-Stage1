  <script>
var myCenteri=new google.maps.LatLng(12.7263582,77.8244447);
function initialize()
{
  var mapProp = {
  center:myCenteri,
  zoom:12,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };
var map=new google.maps.Map(document.getElementById("googleMapindia"),mapProp);
var marker=new google.maps.Marker({
  position:myCenteri,
  });
marker.setMap(map);
}
google.maps.event.addDomListener(window, 'load', initialize);
</script>

<script>
var myCenterf=new google.maps.LatLng(48.8965446,2.0907463,15);
function initialize()
{
  var mapProp1 = {
  center:myCenterf,
  zoom:12,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };
var mapf=new google.maps.Map(document.getElementById("googleMapfrance"),mapProp1);
var markerf=new google.maps.Marker({
  position:myCenterf,
  });
markerf.setMap(mapf);
}
google.maps.event.addDomListener(window, 'load', initialize);
</script>



